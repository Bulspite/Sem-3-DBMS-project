create table Contry_tab
(contry_ID int primary key,
Contry_Name varchar(30) not null,
Company_ID int,   
foreign key (Auditor_SN)
	references com_data(Auditor_SN)
);

insert into Contry_tab (contry_ID, Contry_Name)
values(100, 'India');

