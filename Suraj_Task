
create database Client_data;
use Client_data;


create table com_data
(Auditor_SN int, 
Company_ID int,  
Type_ varchar(25),
Company_Name varchar(35)
);

insert into com_data (Auditor_SN, Company_ID, Type_, Company_Name)
values(101, 101, 'Premium','Google');

